{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // "linebreak-style": "off",
        "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
        // "tab-width": 4,
        "max-len": ["error", {"code": 100}],
        "indent": ["error", 4]
    }
}
